D:/3eme_annee/bal_branche/daval-stock-app/#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,/,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/@mapbox/node-pre-gyp/bin/@mapbox/node-pre-gyp/bin/node-pre-gyp" "$@"
fi
